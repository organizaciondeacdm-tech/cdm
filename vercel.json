{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "^/static/(.*)$",
      "dest": "dist/static/$1"
    },
    {
      "src": "^(.*)$",
      "dest": "dist/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
